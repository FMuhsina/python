n1=int(input("Enter first number :"))
n2=int(input("Enter second number :"))
n3=int(input("Enter third number :"))
if n1>n2 and n1>n3 :
    print("the largest among" ,n1,n2,n3,"is ",n1)
elif n2>n3 :
    print("the largest among" ,n1,n2,n3,"is ",n2)
else :
    print("the largest among" ,n1,n2,n3,"is ",n3)
