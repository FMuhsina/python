n=int(input("Enter number of elements of list: "))
lst=[]
print("Enter",n," elements of list : ")
for i in range(n):
    ele=input()
    lst.append(ele)  
print("list before swaping first and last element :",lst)
lst[0],lst[n-1]=lst[n-1],lst[0]
print("list after swaping first and last element :",lst)
